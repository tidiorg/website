{
	"$schema": "https://biomejs.dev/schemas/1.4.1/schema.json",
	"organizeImports": {
		"enabled": true
	},
  "files": {
    "include": [
      "src/**/*.ts"
    ],
    "ignore": [
      "node_modules",
      "dist"
    ]
  },
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	}
}
